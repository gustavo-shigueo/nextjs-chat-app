	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build:1-next": "cross-env NODE_ENV=production next build",
		"build:2-server": "tsc --project tsconfig.server.json",
		"build": "run-s build:*",
		"dev": "run-p dev:*",
		"dev:next": "cross-env NODE_ENV=development NODE_OPTIONS=\"--inspect\" next dev",
