		"generate": "prisma generate",
		"lint": "next lint",
		"start": "cross-env NODE_ENV=production node dist/server/api/prodServer.js",
		"format": "prettier -w \"src/**/*.(ts|tsx|scss)\"",
		"test": "vitest"
	},
